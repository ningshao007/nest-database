# 数据库配置
# 数据库服务器地址
DB_HOST=localhost

# 数据库端口号
DB_PORT=5432

# 数据库用户名 - 用于身份验证的用户
# 例如：postgres, root, admin 等
DB_USERNAME=postgres

# 数据库密码 - 对应用户名的密码
DB_PASSWORD=password

# 数据库名称 - 具体的数据库实例名称
# 例如：nest_database, myapp, test_db 等
DB_DATABASE=nest_database

# 应用配置
# 应用运行端口
PORT=3000

# 运行环境
NODE_ENV=development 